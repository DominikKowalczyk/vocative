{
    "name": "dominikkowalczyk/vocative",
    "description": "Vocative is an open-source PHP library that allows you to decline Polish names in the nominative form to the vocative form.",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.4",
        "ml/json-ld": "^1.2",
        "monolog/monolog": "^2.0",
        "salsify/json-streaming-parser": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Dominikowalczyk\\Vocative\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dominikowalczyk\\Vocative\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
